/* GENERAL
============================== */
body.open .overlay-bg, #wrapper.open .overlay-bg, .content.open .overlay-bg {opacity: .5;visibility: visible;z-index:1001;}
.overlay-bg.open {opacity: .5;visibility: visible;}
.announcement-bar {z-index:99;position:relative;}
.btn:hover {transform: scale(1.05);}
#slider .headline {background:transparent;}
#servicedesk {background:transparent;}
#servicedesk .content-box {background:transparent;}
#reviewWidget {background:transparent;}
#categories .category {background:transparent;}
#ContactForm {max-width: 600px;width: 100%;}
.form-control {background:var(--color-text-field);border-color:var(--color-border-form);color:var(--color-text-field-text);}
.input-group-text {background:var(--color-text-field);border-color:var(--color-border-form);color:var(--color-text-field-text);}
.btn.btn-snapchat {background-color:#FFFC00;color: #fff;}
.bg-footer {background-color:var(--color-bg-footer);}

.usp .usp-nav .usp-item a {display:flex;}
