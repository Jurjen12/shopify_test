/* HEADER 
============================== */
#topnav {background-color:var(--color-bg-topnav);overflow:hidden;padding-top:5px;padding-bottom:5px;position: relative;z-index: 1021;width:100%;}
#topnav .item {color:var(--color-topnav-text);font-size:13px;text-align: center;display:block;padding: 0px;}
#header {z-index: 9;background: #fff;position: relative;width: 100%;}
#header .full-menu a {font-size: 14px;line-height: 1.2;}
#header .nav-icons .nav-item .nav-link.active .menu-text {color:#fff;}
#header .nav-icons .nav-item:hover .dropdown-menu {opacity: 1;display: block;pointer-events: auto;z-index: 9;}

/* SEARCH
============================== */
#header #search #formSearch .autocomplete{background:#FFF;border: 1px solid #eee;margin-top:30px;opacity:0;position:absolute;min-width:300px;margin-left:-1px;margin-right:-1px;visibility:hidden;width:100%;z-index:10;}
#header #search #formSearch .autocomplete:before{background:none repeat scroll 0 0 #FFF;border-left:1px solid #eee;border-top:1px solid #eee;content:"";display:block;height:10px;position:absolute;top:-6px;right:10px;-webkit-transform:rotate(45deg);-moz-transform:rotate(45deg);-ms-transform:rotate(45deg);-o-transform:rotate(45deg);transform:rotate(45deg);width:10px} 
#header #search #formSearch .autocomplete.search{visibility:visible;opacity:1;margin-top:10px}
#header #search #formSearch .autocomplete .title-box.all {padding: 9px 15px;text-decoration: none !important;display:inline-block;font-size: 15px;background: #f9f9f9;font-weight: 600;margin-bottom:15px;}
#header #search #formSearch .autocomplete .available-cats {text-align:left;padding: 10px 15px;width: 100%;}
#header #search #formSearch .autocomplete .available-cats .search-filtercat {border-radius: 15px;display: inline-block;font-size:14px;padding:4px 10px;text-decoration:none;margin-bottom: 5px;}
#header #search #formSearch .autocomplete.noresults .notfound{display:block}
#header #search #formSearch .autocomplete .title-box span {display: inline-block;}
#header #search #formSearch .autocomplete .result-btn:hover {text-decoration:underline;}
#header #search #formSearch .autocomplete .result-btn p {margin: 0px;font-size: 12px;}
#header #search #formSearch .autocomplete .result-btn span {display: inline-block;font-size: 11px;}
#header #search #formSearch .autocomplete .products{margin-top:0!important;}
#header #search #formSearch .autocomplete .products::-webkit-scrollbar {width:5px;}
#header #search #formSearch .autocomplete .products::-webkit-scrollbar-track {background: #f1f1f1;}
#header #search #formSearch .autocomplete .products::-webkit-scrollbar-thumb {background: #aaa;border-radius: 10px;transition: background-color 0.5s ease;}
#header #search #formSearch .autocomplete .products::-webkit-scrollbar-thumb:hover {background: #aaa;border-radius: 10px;}
#header #search #formSearch .autocomplete .products .search-product {padding: 10px;}
#header #search #formSearch .autocomplete .products .search-product:not(:last-child) {border-bottom:1px solid #f7f7f7;}
#header #search #formSearch .autocomplete .products .search-product .search-product-image img {border:1px solid #eee;}
#header #search #formSearch .autocomplete .products .search-product .search-product-info strong {font-size:14px;line-height: 1.5;display:block;overflow: hidden;text-overflow: ellipsis;white-space:nowrap;}
#header #search #formSearch .autocomplete .products .search-product .search-product-info .price {font-size:14px;}
#header #search #formSearch .autocomplete .products .search-product .search-product-info .price .price-old {color: #aaa;text-decoration: line-through;}
#header #search #formSearch .autocomplete .products .search-product .search-product-info .price .price-tax {display: block;}
#header #search #formSearch .autocomplete .products .search-product .dropdown-cart-details{display:table-cell;vertical-align:middle}
#header #search #formSearch .autocomplete .products .search-product .dropdown-cart-details .item-name {float: left;font-size: 14px;height:44px;line-height: 1.5;display: -webkit-box;overflow: hidden;text-overflow: ellipsis;-webkit-line-clamp: 2;-webkit-box-orient: vertical;width: 100%;margin: 0px;}
#header #search #formSearch .autocomplete.search .more {display:block;}

/* NAVIGATION
============================== */
#navbar {overflow:visible;}
#navbar .navbar {display:flex;background:var(--color-bg-navbar);}
#navbar .navbar .nav-item .nav-link {color:var(--color-navbar-text);}
#navbar .navbar .nav .nav-item > .nav-link {line-height:36px;text-transform: none;}
#navbar .navbar .nav .nav-item > .nav-link::after {background: var(--color-navbar-text);content: '';display: block;position: relative;bottom: -4px;height: 2px;width:0;}
#navbar .navbar .nav .nav-item > .nav-link:hover::after {width: 100%;}
#navbar .navbar .nav-item .submenu {position: absolute;left: 0;right: 0;background: #fff;border-bottom: 1px solid #eee;padding-top: 15px;padding-bottom:15px;width: 100%;z-index: 9;-webkit-transition: all .25s ease;-moz-transition: all .25s ease;-ms-transition: all .25s ease;-o-transition: all .25s ease;transition: all .25s ease;}
#navbar .navbar .nav-item:hover .submenu {opacity:1;visibility:visible;}
#navbar .navbar .nav .nav-item .submenu .subinner > ul {-webkit-columns: 5;-moz-columns: 5;columns: 5;float: left;width: 100%;}
#navbar .navbar .nav .nav-item .submenu ul li {height: 100%;display: inline-block;width: 100%;}
#navbar .navbar .nav .nav-item .submenu ul li a {color:var(--color-body-text);}
#navbar .navbar .nav .nav-item .submenu ul li a:hover {color:var(--color-primary);}

/* FOOTER
============================== */
#shopify-section-footer .index-section--newsletter-background {background:var(--color-bg-newsletter);color:var(--color-text-newsletter);}
#shopify-section-footer .index-section--newsletter-background .newsletter-card-title,
#shopify-section-footer .index-section--newsletter-background a {color:var(--color-text-newsletter);font-weight:var(--font-weight-header);}
#shopify-section-footer .index-section--newsletter-background strong {font-weight:var(--font-weight-header);}
#shopify-section-footer .index-section--newsletter-background .account-content p,
#shopify-section-footer .index-section--newsletter-background .contact-content p {font-size:14px;line-height: 1.6;margin-bottom:0;}
#footer {color: var(--color-footer-text);}
#footer .shape-style-1 {background: var(--color-bg-footer) !important;}
#footer .widget-title {color: var(--color-footer-text);font-weight:var(--font-weight-header);letter-spacing: .06rem;}
#footer .widget-text .content-box p {font-size:14px;}
#footer .widget-list {margin: 0;padding: 0;list-style: none;}
#footer .widget-list .widget-list-item {display:block;font-size:14px;line-height:1.8;margin-bottom: .375rem;}
#footer .widget-list .widget-list-item .widget-list-link {color:var(--color-footer-text);display: block;transition: color 0.25s ease-in-out;font-family: var(--font-stack-body);font-size:var(--font-size-base);font-weight: normal;padding:0;text-transform: none;transition: .2s ease-in-out;}
#footer .widget-list .widget-list-item .widget-list-link:hover {text-decoration:underline;}
#footer .social-media .social-icons .social-icons_item .social-icons_link {color:#fff;display:block;padding:0;margin-right:15px;}

/* SLIDER
============================== */
#slider.two_slides .headline .slide img {opacity:1;-webkit-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);-moz-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);-o-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);}
#slider.two_slides .headline .slide:hover img {opacity:.9;transform: scale(1.05, 1.05);-ms-transform: scale(1.1, 1.1);-webkit-transform: scale(1.05, 1.05);}

.usp .usp-nav .usp-item,
.usp .usp-nav .usp-item p {font-size:14px;margin:0;}

#banners .banner {position:relative;}
#banners .banner .banner-inner {background:#f7f7f7;display:block;overflow:hidden;}
#banners .banner .banner-content {position: absolute;left: 30px;bottom: 30px;z-index: 1;}
#banners .banner .banner-inner a img {-webkit-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);-moz-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);-o-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);}
#banners .banner:hover .banner-inner a img {transform: scale(1.15, 1.15);-ms-transform: scale(1.15, 1.15);-webkit-transform: scale(1.15, 1.15);}

#servicedesk .title-box strong {font-weight:var(--font-weight-header);}
#servicedesk .title-box .service-status span {letter-spacing: 0.03rem;font-weight: normal;text-transform: none;}
#servicedesk .content-box ul li .icon-shape-info {-webkit-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);-moz-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);-o-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);}
#servicedesk .content-box ul li a:hover .icon-shape-info {background-color:var(--color-primary-darker);color:#fff;}
#servicedesk .content-box ul li .icon-content {-webkit-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);-moz-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);-o-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);}
#servicedesk .content-box .pagination {position: absolute;top: 50%;right: 15px;margin: 0px;margin-top: -18px;z-index: 9;}
#reviewWidget {border: .0625rem solid #e9ecef;}
#reviewWidget .ism-contentbox {height:61px;}
#reviewWidget .ism-contentbox .ism-stars i.fa-star {color: #FFC108;}
#reviewWidget .ism-contentbox a .icon-shape-info {-webkit-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);-moz-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);-o-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);}
#reviewWidget .ism-contentbox a:hover .icon-shape-info {background-color: rgba(65, 215, 242, 1.0);color:#fff;}

/* ELEMENTS
============================== */
.usp .usp-nav.with_desc {padding: 30px;margin-top:30px;margin-bottom:30px;}
#categories .categoryborder {margin:0;float:none;}
#categories .category .category-image {background:var(--color-btn-primary-lighter);display:block;overflow:hidden;position:relative;height:100%;width:100%;}
#categories .category .category-image.image-present {background:transparent;}
#categories .category .category-image img {-webkit-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);-moz-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);-o-transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);transition: all .4s cubic-bezier(0.17, 0.04, 0.03, 0.94);}
#categories .category:hover .category-image img {transform: scale(1.15, 1.15);-ms-transform: scale(1.15, 1.15);-webkit-transform: scale(1.15, 1.15);}
#categories .category .category-details {display:block;position:absolute;left:30px;bottom:30px;}
.productborder .product .spr-badge-starrating {margin: 0 3px 0 0;display: block;font-size: 14px;margin-bottom: 7px;}
.productborder .product .spr-badge-caption {display:none;}
.productborder .product-image .label.label-sale {background-color:var(--color-sale-text);}
.productborder:hover .product .thumbnails {opacity: 1;visibility: visible;left: -95px;-webkit-box-shadow: 0 0 6px 0 rgb(0 0 0 / 10%);box-shadow: 0 0 6px 0 rgb(0 0 0 / 10%);-webkit-transform: none;transform: none;display: block;border-right: 0px;z-index: 7;}
.productborder .product .thumbnails .thumb {padding: 5px 0px;position: relative;text-align: center;font-size: 24px;line-height: 60px;}
.productborder .product-overlay {position: absolute;pointer-events: none;z-index: 0;top: 0;right: 0;width: 100%;height: 100%;box-sizing: border-box;opacity: 0;-webkit-transform: scale(1, 1);transform: scale(1, 1);-webkit-transform-origin: top center;transform-origin: top center;transition: opacity 0.2s ease-out, -webkit-transform 0.2s ease-out;transition: opacity 0.2s ease-out, transform 0.2s ease-out;transition: opacity 0.2s ease-out, transform 0.2s ease-out, -webkit-transform 0.2s ease-out;}
.productborder:hover .product-overlay {-webkit-box-shadow: 0 0 6px 0 rgb(0 0 0 / 10%);box-shadow: 0 0 6px 0 rgb(0 0 0 / 10%);border-left: 0px;-webkit-transform: none;transform: none;opacity: 1;}
.productborder .product .thumbnails::after {content: "";background: #fff;width: 5px;height: 100%;display: block;position: absolute;top: 0;right: -5px;}

#blogs .blog-container .article-item p {font-size:14px;}

/* Collectionpage */
.sidebar .widget-categories .title-box svg {cursor:pointer;}
.collection-sidebar__content .collection-sidebar__filter-item.active .custom-checkbox .custom-control-input~.custom-control-label::before {border-color: var(--color-primary);background-color: var(--color-primary);}
.collection-sidebar__content .collection-sidebar__filter-item.active .custom-checkbox .custom-control-input~.custom-control-label::after {background-image:url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 8 8'%3e%3cpath fill='%23fff' d='M6.564.75l-3.59 3.612-1.538-1.55L0 4.26 2.974 7.25 8 2.193z'/%3e%3c/svg%3e");}

/* Productpage */
#thumbs-desktop::-webkit-scrollbar {width:8px;}
#thumbs-desktop::-webkit-scrollbar-track {background: #fff;}
#thumbs-desktop::-webkit-scrollbar-thumb {background: #ddd;border-radius: 10px;transition: background-color 0.5s ease;}
#thumbs-desktop::-webkit-scrollbar-thumb:hover {background: #ccc;border-radius: 10px;}
#thumbs-desktop {max-height: 440px;overflow-y: auto;padding-right: 15px;}
.product-page .product-title .spr-badge {display: inline-block;vertical-align: middle;}
#product-price .price-new {font-size:24px;font-weight:bold;}
#product-price .price-old {color: #ccc;text-decoration: line-through;}
#AddToCartForm .selector-wrapper select {border: 1px solid #eee;}
.ism-product-countdown b,
.combi-list .combi-item b {font-weight:600;}
.product-bottom .product-tab .content-box p, 
.product-bottom .product-tab .content-box li {font-size: 14px;}
#specs table tr td {border:0 solid !important;}
#specs table tr:nth-child(odd) {background:#f7f7f7;}
.spr-container {padding: 0;border-color: transparent;}
.related-slider .productborder .product-overlay, .related-slider .productborder .thumbnails {display:none !important;}

/* Cart Page */
.widget {-webkit-box-shadow: 0 0 6px 0 rgb(0 0 0 / 10%);box-shadow: 0 0 6px 0 rgb(0 0 0 / 10%);}

